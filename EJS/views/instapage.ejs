<%- include("includes/head.ejs")%>
<body>
    <h2>This page belongs to @<%= json.name %></h2>
    <button>Follow</button>
    <button>Message</button>
    <p>Followers : <%=json.followers%></p>
    <p>Following : <%=json.following%></p>
    <hr>
    <% for(i of json.posts){%>
        <div>
            <img src=<%= i.image %> height=200px width=200px >
            <p>Likes : <%=i.likes%></p>
            <p>Comments : <%=i.comments%></p>
        </div>
    <%}%>
</body>
</html>